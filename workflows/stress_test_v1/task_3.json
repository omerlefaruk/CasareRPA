{
  "metadata": {
    "name": "Math Calculation Workflow",
    "description": "Calculates (10 * 2) + (50 / 2) - 5 using serial MathOperationNodes.",
    "version": "1.0.0",
    "author": "",
    "tags": []
  },
  "nodes": {
    "multiply_step": {
      "node_id": "multiply_step",
      "node_type": "MathOperationNode",
      "config": {
        "a": 10,
        "b": 2,
        "operation": "multiply"
      },
      "position": [
        0.0,
        0.0
      ]
    },
    "divide_step": {
      "node_id": "divide_step",
      "node_type": "MathOperationNode",
      "config": {
        "a": 50,
        "b": 2,
        "operation": "divide"
      },
      "position": [
        400.0,
        0.0
      ]
    },
    "add_step": {
      "node_id": "add_step",
      "node_type": "MathOperationNode",
      "config": {
        "a": "{{multiply_step.result}}",
        "b": "{{divide_step.result}}",
        "operation": "add"
      },
      "position": [
        800.0,
        0.0
      ]
    },
    "subtract_step": {
      "node_id": "subtract_step",
      "node_type": "MathOperationNode",
      "config": {
        "a": "{{add_step.result}}",
        "b": 5,
        "operation": "subtract"
      },
      "position": [
        1200.0,
        0.0
      ]
    },
    "log_result": {
      "node_id": "log_result",
      "node_type": "LogNode",
      "config": {
        "message": "The final result is: {{subtract_step.result}}"
      },
      "position": [
        1600.0,
        0.0
      ]
    }
  },
  "connections": [
    {
      "source_node": "multiply_step",
      "source_port": "exec_out",
      "target_node": "divide_step",
      "target_port": "exec_in"
    },
    {
      "source_node": "divide_step",
      "source_port": "exec_out",
      "target_node": "add_step",
      "target_port": "exec_in"
    },
    {
      "source_node": "add_step",
      "source_port": "exec_out",
      "target_node": "subtract_step",
      "target_port": "exec_in"
    },
    {
      "source_node": "subtract_step",
      "source_port": "exec_out",
      "target_node": "log_result",
      "target_port": "exec_in"
    }
  ],
  "variables": {},
  "settings": {
    "stop_on_error": true,
    "timeout": 30,
    "retry_count": 0
  }
}
