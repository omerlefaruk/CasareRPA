{
    "metadata": {
        "name": "Multi-Agent Chain Example",
        "description": "Demonstrates chaining multiple AI agents where each agent's output feeds into the next. Pattern: Researcher → Analyzer → Summarizer",
        "version": "1.0.0",
        "author": "CasareRPA AI",
        "tags": [
            "ai",
            "multi-agent",
            "chain",
            "llm",
            "demo"
        ]
    },
    "nodes": {
        "0": {
            "node_id": "0",
            "node_type": "AIAgentNode",
            "config": {
                "goal": "Research and gather information about the topic: {{topic}}. Find key facts, statistics, and important details. Return your findings as a structured JSON with 'facts', 'sources', and 'key_points' fields.",
                "context": "{\"role\": \"researcher\", \"topic\": \"{{topic}}\"}",
                "max_steps": 5,
                "timeout": 120,
                "model": "gpt-4o-mini"
            },
            "position": [
                0,
                0
            ]
        },
        "1": {
            "node_id": "1",
            "node_type": "AIAgentNode",
            "config": {
                "goal": "Analyze the research findings provided in the context. Identify patterns, draw insights, and create an analysis report. Structure your output as JSON with 'insights', 'patterns', 'recommendations' fields.",
                "context": "{{node_0_result}}",
                "max_steps": 5,
                "timeout": 120,
                "model": "gpt-4o-mini"
            },
            "position": [
                500,
                0
            ]
        },
        "2": {
            "node_id": "2",
            "node_type": "AIAgentNode",
            "config": {
                "goal": "Create a final executive summary from the analysis. Make it concise, actionable, and suitable for presentation. Include key takeaways and next steps. Output as JSON with 'summary', 'key_takeaways', 'next_steps' fields.",
                "context": "{{node_1_result}}",
                "max_steps": 3,
                "timeout": 60,
                "model": "gpt-4o-mini"
            },
            "position": [
                1000,
                0
            ]
        }
    },
    "connections": [
        {
            "source_node": "0",
            "source_port": "exec_out",
            "target_node": "1",
            "target_port": "exec_in"
        },
        {
            "source_node": "0",
            "source_port": "result",
            "target_node": "1",
            "target_port": "context"
        },
        {
            "source_node": "1",
            "source_port": "exec_out",
            "target_node": "2",
            "target_port": "exec_in"
        },
        {
            "source_node": "1",
            "source_port": "result",
            "target_node": "2",
            "target_port": "context"
        }
    ],
    "variables": {
        "topic": "Artificial Intelligence in Healthcare"
    },
    "settings": {
        "stop_on_error": true,
        "timeout": 600,
        "retry_count": 1
    }
}
