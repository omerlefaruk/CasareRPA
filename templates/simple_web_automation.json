{
    "metadata": {
        "name": "Simple Web Automation",
        "description": "Opens a browser, navigates to Google, waits, and closes. Created using CasareRPA infrastructure/AI smartagent workflow patterns.",
        "version": "1.0.0",
        "author": "CasareRPA AI",
        "tags": [
            "web",
            "browser",
            "automation",
            "demo"
        ]
    },
    "nodes": {
        "0": {
            "node_id": "0",
            "node_type": "LaunchBrowserNode",
            "config": {
                "url": "https://www.google.com",
                "browser_type": "chromium",
                "headless": false,
                "window_state": "maximized",
                "do_not_close": false,
                "profile_mode": "none",
                "viewport_width": 1280,
                "viewport_height": 720
            },
            "position": [
                0,
                0
            ]
        },
        "1": {
            "node_id": "1",
            "node_type": "WaitNode",
            "config": {
                "duration_ms": 2000
            },
            "position": [
                400,
                0
            ]
        },
        "2": {
            "node_id": "2",
            "node_type": "WaitForElementNode",
            "config": {
                "selector": "textarea[name='q']",
                "timeout": 10000
            },
            "position": [
                800,
                0
            ]
        },
        "3": {
            "node_id": "3",
            "node_type": "ClickNode",
            "config": {
                "selector": "textarea[name='q']",
                "wait_for_visible": true,
                "timeout": 5000
            },
            "position": [
                1200,
                0
            ]
        },
        "4": {
            "node_id": "4",
            "node_type": "TypeNode",
            "config": {
                "selector": "textarea[name='q']",
                "text": "CasareRPA Automation",
                "delay": 50
            },
            "position": [
                1600,
                0
            ]
        },
        "5": {
            "node_id": "5",
            "node_type": "WaitNode",
            "config": {
                "duration_ms": 1500
            },
            "position": [
                2000,
                0
            ]
        },
        "6": {
            "node_id": "6",
            "node_type": "CloseBrowserNode",
            "config": {
                "timeout": 5000,
                "force_close": false
            },
            "position": [
                2400,
                0
            ]
        }
    },
    "connections": [
        {
            "source_node": "0",
            "source_port": "exec_out",
            "target_node": "1",
            "target_port": "exec_in"
        },
        {
            "source_node": "1",
            "source_port": "exec_out",
            "target_node": "2",
            "target_port": "exec_in"
        },
        {
            "source_node": "2",
            "source_port": "exec_out",
            "target_node": "3",
            "target_port": "exec_in"
        },
        {
            "source_node": "3",
            "source_port": "exec_out",
            "target_node": "4",
            "target_port": "exec_in"
        },
        {
            "source_node": "4",
            "source_port": "exec_out",
            "target_node": "5",
            "target_port": "exec_in"
        },
        {
            "source_node": "5",
            "source_port": "exec_out",
            "target_node": "6",
            "target_port": "exec_in"
        }
    ],
    "variables": {},
    "settings": {
        "stop_on_error": true,
        "timeout": 120,
        "retry_count": 0
    }
}
