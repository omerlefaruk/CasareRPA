{
    "metadata": {
        "name": "AI Notepad Automation",
        "description": "Opens Windows Notepad, types a multi-line message with timestamps, and closes gracefully. Generated using CasareRPA infrastructure/AI SmartWorkflowAgent patterns with PLAN -> BUILD -> VALIDATE -> REPAIR -> OUTPUT protocol.",
        "version": "1.0.0",
        "author": "CasareRPA AI SmartWorkflowAgent",
        "tags": [
            "notepad",
            "desktop",
            "automation",
            "ai-generated",
            "demo"
        ],
        "ai_config": {
            "wait_strategy": "minimal_waits",
            "error_handling": "critical_only",
            "parallelization": "sequential"
        }
    },
    "nodes": {
        "0": {
            "node_id": "0",
            "node_type": "LaunchApplicationNode",
