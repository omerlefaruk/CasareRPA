                0
            ]
        },
        "2": {
            "node_id": "2",
            "node_type": "SendKeysNode",
            "config": {
                "keys": "=== CasareRPA AI Workflow Demo ===",
                "interval": 0.02
            },
            "position": [
                800,
                0
            ]
        },
        "3": {
            "node_id": "3",
            "node_type": "SendHotKeyNode",
