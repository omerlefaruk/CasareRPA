{
    "metadata": {
        "name": "Calculator to Notepad",
        "description": "Opens Windows Calculator, performs a calculation, copies the result, and pastes it into Notepad.",
        "version": "1.0.0",
        "author": "CasareRPA",
        "tags": [
            "calculator",
            "notepad",
            "desktop",
            "automation",
            "clipboard"
        ]
    },
    "nodes": {
        "launch_calculator": {
            "node_id": "launch_calculator",
            "node_type": "LaunchApplicationNode",
            "config": {
                "application_path": "calc.exe",
                "window_title_hint": "Calculator",
                "timeout": 10.0,
                "keep_open": true
            },
            "position": [
                0,
                0
            ]
        },
        "wait_calc_ready": {
            "node_id": "wait_calc_ready",
            "node_type": "WaitNode",
            "config": {
                "duration_ms": 1500
            },
            "position": [
                400,
                0
            ]
        },
        "type_first_number": {
            "node_id": "type_first_number",
            "node_type": "SendKeysNode",
            "config": {
                "keys": "25",
                "interval": 0.05
            },
            "position": [
                800,
                0
            ]
        },
        "type_plus": {
            "node_id": "type_plus",
            "node_type": "SendKeysNode",
            "config": {
                "keys": "+",
                "interval": 0.0
            },
            "position": [
                1200,
                0
            ]
        },
        "type_second_number": {
            "node_id": "type_second_number",
            "node_type": "SendKeysNode",
            "config": {
                "keys": "17",
                "interval": 0.05
            },
            "position": [
                1600,
                0
            ]
        },
        "press_equals": {
            "node_id": "press_equals",
            "node_type": "SendHotKeyNode",
            "config": {
                "modifier": "none",
                "key": "Enter"
            },
            "position": [
                2000,
                0
            ]
        },
        "wait_for_result": {
            "node_id": "wait_for_result",
            "node_type": "WaitNode",
            "config": {
                "duration_ms": 500
            },
            "position": [
                2400,
                0
            ]
        },
        "copy_result": {
            "node_id": "copy_result",
            "node_type": "SendHotKeyNode",
            "config": {
                "modifier": "ctrl",
                "key": "c"
            },
            "position": [
                2800,
                0
            ]
        },
        "wait_after_copy": {
            "node_id": "wait_after_copy",
            "node_type": "WaitNode",
            "config": {
                "duration_ms": 300
            },
            "position": [
                3200,
                0
            ]
        },
        "launch_notepad": {
            "node_id": "launch_notepad",
            "node_type": "LaunchApplicationNode",
            "config": {
                "application_path": "notepad.exe",
                "window_title_hint": "Notepad",
                "timeout": 10.0,
                "keep_open": true
            },
            "position": [
                3600,
                0
            ]
        },
        "wait_notepad_ready": {
            "node_id": "wait_notepad_ready",
            "node_type": "WaitNode",
            "config": {
                "duration_ms": 1000
            },
            "position": [
                4000,
                0
            ]
        },
        "type_header": {
            "node_id": "type_header",
            "node_type": "SendKeysNode",
            "config": {
                "keys": "Calculator Result: ",
                "interval": 0.02
            },
            "position": [
                4400,
                0
            ]
        },
        "paste_result": {
            "node_id": "paste_result",
            "node_type": "SendHotKeyNode",
            "config": {
                "modifier": "ctrl",
                "key": "v"
            },
            "position": [
                4800,
                0
            ]
        },
        "type_footer": {
            "node_id": "type_footer",
            "node_type": "SendKeysNode",
            "config": {
                "keys": " (25 + 17)",
                "interval": 0.02
            },
            "position": [
                5200,
                0
            ]
        }
    },
    "connections": [
        {
            "source_node": "launch_calculator",
            "source_port": "exec_out",
            "target_node": "wait_calc_ready",
            "target_port": "exec_in"
        },
        {
            "source_node": "wait_calc_ready",
            "source_port": "exec_out",
            "target_node": "type_first_number",
            "target_port": "exec_in"
        },
        {
            "source_node": "type_first_number",
            "source_port": "exec_out",
            "target_node": "type_plus",
            "target_port": "exec_in"
        },
        {
            "source_node": "type_plus",
            "source_port": "exec_out",
            "target_node": "type_second_number",
            "target_port": "exec_in"
        },
        {
            "source_node": "type_second_number",
            "source_port": "exec_out",
            "target_node": "press_equals",
            "target_port": "exec_in"
        },
        {
            "source_node": "press_equals",
            "source_port": "exec_out",
            "target_node": "wait_for_result",
            "target_port": "exec_in"
        },
        {
            "source_node": "wait_for_result",
            "source_port": "exec_out",
            "target_node": "copy_result",
            "target_port": "exec_in"
        },
        {
            "source_node": "copy_result",
            "source_port": "exec_out",
            "target_node": "wait_after_copy",
            "target_port": "exec_in"
        },
        {
            "source_node": "wait_after_copy",
            "source_port": "exec_out",
            "target_node": "launch_notepad",
            "target_port": "exec_in"
        },
        {
            "source_node": "launch_notepad",
            "source_port": "exec_out",
            "target_node": "wait_notepad_ready",
            "target_port": "exec_in"
        },
        {
            "source_node": "wait_notepad_ready",
            "source_port": "exec_out",
            "target_node": "type_header",
            "target_port": "exec_in"
        },
        {
            "source_node": "type_header",
            "source_port": "exec_out",
            "target_node": "paste_result",
            "target_port": "exec_in"
        },
        {
            "source_node": "paste_result",
            "source_port": "exec_out",
            "target_node": "type_footer",
            "target_port": "exec_in"
        }
    ],
    "variables": {},
    "settings": {
        "stop_on_error": true,
        "timeout": 120,
        "retry_count": 0
    }
}
