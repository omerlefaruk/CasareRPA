    "save_invoice_pdf": {
      "node_id": "save_invoice_pdf",
      "node_type": "BrowserRunScriptNode",
      "position": [
        7585.10091547049,
        -283.1447920858827
      ],
      "config": {
        "script": "\nimport os\nfrom datetime import datetime\n\n# Configure paths\ndest_dir = r\"C:\\Users\\Rau\\Desktop\\muhasebe\"\nos.makedirs(dest_dir, exist_ok=True)\n\nproject_name = \"MonthlyMuhasebe\"\naccount_num = \"4007119742\"\nfilename = f\"{project_name}_Fatura_{account_num}_{datetime.now().strftime('%Y%m%d_%H%M%S')}.pdf\"\nfull_path = os.path.join(dest_dir, filename)\n\nlogger.info(f\"Target PDF path: {full_path}\")\n\n# Smart wait: Wait for network to be idle (PDF viewer fully loaded)\nlogger.info(\"Waiting for PDF viewer to load...\")\ntry:\n    await page.wait_for_load_state('networkidle', timeout=30000)\nexcept:\n    pass\n\ndownloaded = False\n\n# The PDF viewer download button is in Chrome's Shadow DOM (not accessible via normal selectors)\n# We'll click it by position - the download button is in the PDF viewer toolbar\n# Typical Chrome PDF viewer: toolbar is at top, download icon is near right side\n\n# Get viewport size\nviewport = page.viewport_size\nlogger.info(f\"Viewport size: {viewport}\")\n\n# Download button position in Chrome PDF viewer:\n# - It's in the top toolbar (y ~ 20-25 pixels from top)\n# - It's the second button from the right (before the menu icon)\n# - Approximately 60-80 pixels from the right edge\n\ndownload_x = viewport['width'] - 70  # ~70px from right edge\ndownload_y = 22  # In the toolbar area\n\nlogger.info(f\"Clicking download button at position ({download_x}, {download_y})\")\n\ntry:\n    async with page.expect_download(timeout=60000) as download_info:\n        # Click at the download button position\n        await page.mouse.click(download_x, download_y)\n        \n        logger.info(\"Waiting for download to complete...\")\n        download = await download_info.value\n        await download.save_as(full_path)\n        downloaded = True\n        logger.info(f\"SUCCESS: Downloaded PDF to {full_path}\")\n\nexcept Exception as e:\n    logger.error(f\"Download at ({download_x}, {download_y}) failed: {e}\")\n    \n    # Try alternative position (print button is next to download)\n    if not downloaded:\n        try:\n            print_x = viewport['width'] - 40  # Closer to edge (might be print)\n            logger.info(f\"Trying alternative position ({print_x}, {download_y})\")\n            \n            async with page.expect_download(timeout=30000) as download_info:\n                await page.mouse.click(print_x, download_y)\n                download = await download_info.value\n                await download.save_as(full_path)\n                downloaded = True\n                logger.info(f\"SUCCESS: Downloaded PDF to {full_path}\")\n        except Exception as e2:\n            logger.error(f\"Alternative position failed: {e2}\")\n\n# FAIL if not downloaded - NO fallbacks\nif not downloaded:\n    raise Exception(f\"FAILED: Could not download PDF by clicking toolbar. Check button position.\")\n\nresult = full_path\n",
        "timeout": "120000",
        "retry_count": "0",
        "retry_interval": "1000",
        "status": "idle"
      },
      "name": "Save Invoice PDF"
    },
